<div id="map">MAP GOES HERE</div>
<div id="address" class="vcard">
  <h3 class="fn org"><%= @name %></h3>
  <div class="adr">
    <div class="street-address"><%= @address.street %></div>
    <span class="locality">Durham</span>,
    <abbr class="region" title="North Carolina">NC</abbr>
    <span class="postal-code"><%= @address.zip %></span>
  </div>
  <div class="tel"><%= @phone %></div>
</div>
